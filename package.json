{
  "name": "events-server",
  "version": "1.0.0",
  "description": "api to create events occurs from 9AM to 9PM",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "server": "tsc && node ./dist/server.js",
    "dev:server": "tsc --project './tsconfig.json' --watch & NODE_ENV=development nodemon ./dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "clean": "rimraf -rf dist node_modules package-lock.json",
    "lint": "tslint -c './tslint.json' -p './tsconfig.json' --fix"
  },
  "keywords": [
    "typescript",
    "express",
    "api",
    "events"
  ],
  "author": "shushanth",
  "license": "MIT",
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.2",
    "@types/supertest": "^2.0.8",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^7.0.2",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.7.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "swagger-ui-express": "^4.1.3",
    "uuid": "^7.0.2",
    "winston": "^3.2.1"
  }
